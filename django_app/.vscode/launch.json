{
    "configurations": [
        {
            "name": "Redbox",
            "type": "debugpy",
            "request": "launch",
            "module": "daphne",
            "args": [
                "redbox_app.asgi:application",
                "--port", "8080",
            ],
            "envFile": "${workspaceFolder}/../.env",
            "django": true,
            "env": {
                "MINIO_HOST": "localhost",
                "POSTGRES_HOST": "localhost",
                "UNSTRUCTURED_HOST": "localhost",
                "ELASTIC__HOST": "localhost"
            }
        },
        
        {
            "name": "Worker",
            "type": "debugpy",
            "request": "launch",
            "gevent": true,
            "program": "venv/bin/django-admin",
            "args": [
                "qcluster",
            ],
            "envFile": "${workspaceFolder}/../.env",
            "env": {
                "MINIO_HOST": "localhost",
                "POSTGRES_HOST": "localhost",
                "UNSTRUCTURED_HOST": "localhost",
                "ELASTIC__HOST": "localhost"
            }
        },
    ]
}